{
  "name": "@frosti/react",
  "version": "0.2.0",
  "description": "Boilerplate React App",
  "author": "Jesse Weed <code@jesseweed.com>",
  "license": "MIT",
  "homepage": "http://frosti.js.org",
  "repository": "https://github.com/frostijs/react",
  "bugs": "https://github.com/frostijs/react/issues",
  "engines": {
    "node": "10.15.x"
  },
  "scripts": {
    "build": "run-p build:client build:server",
    "build:client": "npm run rollup:prod",
    "build:server": "npm run rollup:server",
    "build:dev": "npm run rollup:watch",
    "check": "run-s check:unused check:security",
    "check:unused": "node lib/util/depcheck",
    "check:security": "npm audit",
    "copy": "node lib/util/copy",
    "deploy": "run-p copy deploy:firebase",
    "deploy:firebase": "NODE_ENV=production firebase deploy --only functions,hosting",
    "dev": "run-s build:dev start:ssr",
    "docker": "run-s docker:build docker:start",
    "docker:build": "docker build -t frosti-react ./",
    "docker:start": "docker run -it -p 3000:3000 frosti-react",
    "lint": "eslint src --ext .js,.jsx --ignore-path .gitignore .",
    "prettier": "pretty-quick",
    "reload": "livereload '.dist/' -d",
    "rollup:prod": "NODE_ENV=production rollup -c config/rollup/rollup.config.js",
    "rollup:server": "NODE_ENV=production rollup -c config/rollup/rollup.config.server.js",
    "rollup:watch": "NODE_ENV=development rollup -c config/rollup/rollup.config.js -w",
    "start:heroku": "HOST=heroku node ./functions/.dist/ssr.js",
    "start:docker": "HOST=docker node ./functions/.dist/ssr.js",
    "start:node": "HOST=local babel-node src/Express.js",
    "start:ssr": "HOST=ssr node ./functions/.dist/ssr.js",
    "ssl": "nodecert",
    "test": "jest --config ./config/jest.config.js",
    "vet": "run-p prettier check test"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run vet"
    }
  },
  "dependencies": {
    "chokidar": "^2.1.5",
    "compression": "^1.7.4",
    "colors": "^1.3.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^5.0.0",
    "redirect-https": "^1.3.0",
    "spdy": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@csstools/postcss-sass": "^4.0.0",
    "@ind.ie/nodecert": "^3.0.1",
    "babel-eslint": "^10.0.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-transform-dynamic-import": "^2.1.0",
    "babel-plugin-transform-postcss": "^0.3.0",
    "depcheck": "^0.8.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "favicons": "^5.3.0",
    "husky": "^2.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.5.0",
    "npm-run-all": "^4.1.5",
    "postcss-easy-import": "^3.0.0",
    "postcss-hexrgba": "^1.0.1",
    "postcss-mixins": "^6.2.1",
    "postcss-preset-env": "^6.6.0",
    "postcss-pxtorem": "^4.0.1",
    "prettier": "^1.17.1",
    "pretty-quick": "^1.10.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.7.4",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-babel-minify": "^8.0.0",
    "rollup-plugin-browsersync": "^1.0.0",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-eslint": "^5.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.1.1",
    "stylus": "^0.54.5"
  }
}
